                    "url"       : "https://github.com/documentcloud/backbone.git",
                    "type"      : "git",
                    "reference" : "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name"    : "jquery/jquery",
                "version" : "master",
                "source"  : {
                    "url"       : "https://github.com/jquery/jquery.git",
                    "type"      : "git",
                    "reference" : "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "swslib/swslib",
                "version": "master",
                "source": {
                    "url": "http://gitosis@git.local/sws-lib.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "php"                           : ">=5.3.2",
        "ajkochanowicz/kickstrap"       : "*",
        "documentcloud/underscore"      : "*",
