        {
            "name"  : "Algis Dumbris",
            "email" : "a.dumbris@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name"    : "twitter/bootstrap",
                "version" : "master",
                "source"  : {
                    "url"       : "https://github.com/twitter/bootstrap.git",
                    "type"      : "git",
                    "reference" : "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name"    : "ajkochanowicz/kickstrap",
                "version" : "master",
                "source"  : {
                    "url"       : "https://github.com/ajkochanowicz/Kickstrap.git",
                    "type"      : "git",
                    "reference" : "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name"    : "modernizr/modernizr",
                "version" : "dev-master",
                "source"  : {
                    "url"       : "https://github.com/Modernizr/Modernizr.git",
                    "type"      : "git",
                    "reference" : "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name"    : "documentcloud/underscore",
                "version" : "master",
                "source"  : {
                    "url"       : "https://github.com/documentcloud/underscore.git",
                    "type"      : "git",
                    "reference" : "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name"    : "documentcloud/backbone",
                "version" : "master",
