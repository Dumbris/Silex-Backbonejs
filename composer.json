        "jquery/jquery"                 : "*",
        "swslib/swslib"                 : "*",
        "silex/silex"                   : "1.0.*",
        "twig/twig"                     : "1.6.2",
        "monolog/monolog"               : ">=1.0.0",
        "symfony/form"                  : "2.1.*",
        "symfony/translation"           : "2.1.*",
        "symfony/twig-bridge"           : "2.1.*",
        "symfony/validator"             : "2.1.*",
        "symfony/yaml"                  : "2.1.*",
        "symfony/config"                : "2.1.*",
        "kriswallsmith/assetic"         : "1.0.x-dev",
        "fate/silex-extensions"         : "dev-master"
    }
}
