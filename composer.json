{
    "name"        : "Dumbris/Silex-backbonejs",
    "type"        : "library",
    "description" : "A Silex Edition",
    "keywords"    : ["framework"],
    "homepage"    : "https://github.com/Dumbris/Silex-backbonejs",
    "license"     : "MIT",
